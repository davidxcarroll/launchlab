extends _base.pug

block content

	div.hero-full.hero-home
		div.nav-home
			include _includes/header.pug

	div.outer.primary-message
		div.inner
			h1.h1
				span.break The science of resort 
				span.break real estate sales

	div.outer.buckets-3-col.primary-message-buckets
		div.inner
			ul
				li
					div.svg.primary-message-svg
						include _svg/bucket-marketing_advertising.pug
					p.h7
						span.break Internet Marketing 
						span.break and Advertising
				li
					div.svg.primary-message-svg
						include _svg/bucket-accelerated_sales.pug
					p.h7
						span.break Accelerated Real 
						span.break Estate Sales
				li
					div.svg.primary-message-svg
						include _svg/bucket-secret_sauce.pug
					p.h7
						span.break Unconventional 
						span.break Secret Sauce

	+project(
	'img/project/project-westgate.jpg',
	'State record',
	'Utah pre-sales',
	'$65M',
	'Pre-sale launch',
	'Westgate Resort, Park City, Utah',
	'Sales Success In Two Economic Eras',
	'project-westgate.html')
	
	include _includes/map.pug

	+testimonial-featured(
	'Launch Lab executed a sales and marketing process for us that exceeded our expectations, generating $100M for us in one year. Just as important, they are true professionals that conduct their business with the utmost level of effort and integrity.',
	'img/client/client-mark_waltrip.jpg',
	'Mark Waltrip',
	'COO Westgate Resorts')

	include _includes/contact-form.pug

	+whitepaper(
	'img/whitepaper/whitepaper-10-point-process-for-digital-marketing.jpg',
	'Our 10-point process for digital marketing',
	'Nunc eu ullamcorper orci. Quisque eget odio ac lectus vestibulum faucibus eget in metus. In pellentesque faucibus vestibulum.',
	'whitepaper-10-point-process-for-digital-marketing.html')





